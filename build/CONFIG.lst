# OS/KERNEL CONFIGURATION LIST
{include/.config.h}
[arch]
ARCH: x86_64
CONFIG_SMP: YES
[debug]
CONFIG_DEBUG: PARTIAL
CONFIG_DEBUG_SERIAL: YES
CONFIG_DEBUG_VERBOSE: NO
[library]
CONFIG_FAST_LIB_ROUTINE: YES
[GUI]
CONFIG_GUI: YES
CONFIG_KERNEL_FONT: YES
[mm]
CONFIG_MAP_ALL_MEMORY: PARTIAL
[misc]
CONFIG_BUILTIN_INTRINSICS: PARTIAL
#{1.mk}
#[toolchain]
#CC: gcc
#CXX: g++ -std=c++11
#LD: ld
#AR: ar
